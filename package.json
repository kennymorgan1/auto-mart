{
  "name": "auto-mart",
  "version": "1.0.0",
  "description": "Automart car sales",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kennymorgan1/auto-mart.git"
  },
  "author": "Kennymorgan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kennymorgan1/auto-mart/issues"
  },
  "homepage": "https://github.com/kennymorgan1/auto-mart#readme",
  "scripts": {
    "build": "babel --out-dir dist server",
    "start": "npm run build && node dist/app.js",
    "dev": "npm run build && nodemon dist/app.js",
    "lint": "eslint server",
    "test": "NODE_ENV=test && babel-node ./node_modules/mocha/bin/_mocha --timeout 20000 server/test/*.js --exit || true",
    "report-coverage": "NODE_ENV=test && babel-node ./node_modules/babel-istanbul/lib/cli cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec server/test/*.js --exit",
    "coveralls": "npm run report-coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "coverage": "NODE_ENV=test && babel-node ./node_modules/babel-istanbul/lib/cli cover --hook-run-in-context node_modules/mocha/bin/_mocha -- -R spec --timeout 10000 server/test/*.js --exit"
  },
  "dependencies": {
    "@sendgrid/mail": "^6.4.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-istanbul": "^0.12.2",
    "babel-loader": "^8.0.4",
    "babel-plugin-istanbul": "^5.1.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-latest": "^6.24.1",
    "babel-register": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "chalk": "^2.4.2",
    "cloudinary": "^1.14.0",
    "cross-env": "^5.2.0",
    "dotenv": "^6.2.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-watch": "^4.0.2",
    "express": "^4.17.0",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^5.2.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "pg": "^7.7.1",
    "swagger-ui-express": "^4.0.5"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "coveralls": "^3.0.3",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.2",
    "nyc": "^14.1.1"
  },
  "nyc": {
    "all": true,
    "check-coverage": true,
    "branches": 100,
    "functions": 100,
    "lines": 100,
    "statements": 100,
    "reporter": [
      "text",
      "lcov"
    ],
    "instrument": false,
    "sourceMap": false,
    "require": [
      "babel-register"
    ],
    "include": [
      "server"
    ]
  }
}
